      <!-- High-conversion CTA + secure form -->
      <section id="collaborate" class="section section-warning" aria-labelledby="ctaTitle">
        <div class="container cta-2">
          <div class="cta2-left reveal-left">
            <h2 id="ctaTitle" class="section-title">SME IPO activity is accelerating</h2>
            <p class="section-lede">
              Promoters prefer advisors who can hand-hold the entire journey. Partner with an execution-led
              team and convert faster—without execution risk.
            </p>
            <div class="cta2-actions">
              <a class="btn btn-primary btn-lg" href="#leadForm">Partner With Us Now</a>
              <a class="btn btn-secondary btn-lg" href="#leadForm">Request a Call</a>
            </div>
            <p class="mini-disclaimer">
              Secure submission. No data exposed publicly. We validate inputs and rate-limit requests.
            </p>
          </div>

          <div class="cta2-right reveal-left">
            <form id="leadForm" class="lead-form" novalidate>
              <div class="form-head">
                <p class="form-title">Collaborate With Us</p>
                <p class="form-sub">Share your details. We’ll respond promptly.</p>
              </div>

              <div class="form-grid">
                <label class="field">
                  <span>Name</span>
                  <input name="name" autocomplete="name" required minlength="2" placeholder="Your full name" />
                  <span class="field-err" data-err-for="name" aria-live="polite"></span>
                </label>
                <label class="field">
                  <span>Firm Name</span>
                  <input name="firm" autocomplete="organization" required minlength="2" placeholder="Your firm" />
                  <span class="field-err" data-err-for="firm" aria-live="polite"></span>
                </label>
                <label class="field">
                  <span>Email</span>
                  <input name="email" autocomplete="email" required type="email" placeholder="you@firm.com" />
                  <span class="field-err" data-err-for="email" aria-live="polite"></span>
                </label>
                <label class="field">
                  <span>Phone</span>
                  <input name="phone" autocomplete="tel" required inputmode="tel" placeholder="+91 9xxxx xxxxx" />
                  <span class="field-err" data-err-for="phone" aria-live="polite"></span>
                </label>
                <label class="field field-full">
                  <span>Partner Type</span>
                  <select name="partnerType" required>
                    <option value="" selected disabled>Select…</option>
                    <option value="ca">CA</option>
                    <option value="cs">CS</option>
                    <option value="consultant">Consultant</option>
                    <option value="advisor">Advisor</option>
                    <option value="other">Other</option>
                  </select>
                  <span class="field-err" data-err-for="partnerType" aria-live="polite"></span>
                </label>
                <label class="field field-full">
                  <span>Message</span>
                  <textarea
                    name="message"
                    required
                    minlength="20"
                    placeholder="Tell us about your client profile and what support you need."
                  ></textarea>
                  <span class="field-err" data-err-for="message" aria-live="polite"></span>
                </label>

                <!-- Honeypot (bot trap) -->
                <label class="hp" aria-hidden="true">
                  <span>Website</span>
                  <input name="website" tabindex="-1" autocomplete="off" />
                </label>
              </div>

              <button class="btn btn-primary btn-lg form-submit" type="submit">
                <span class="btn-label">Submit securely</span>
                <span class="btn-spinner" aria-hidden="true"></span>
              </button>

              <div id="formStatus" class="form-status" role="status" aria-live="polite"></div>
              <p class="form-privacy">
                By submitting, you agree to be contacted about this inquiry. We do not sell your data.
              </p>
            </form>
          </div>
        </div>
      </section>
    </main>

